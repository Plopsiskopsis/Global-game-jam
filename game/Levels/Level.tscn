code = "shader_type canvas_item;

void fragment()
{
	COLOR = texture(TEXTURE, UV);
	if (COLOR.r >= 0.075)
	{
		COLOR = vec4(0.0);
	} else 
	{
		COLOR = texture(TEXTURE, UV) * vec4(sin(TIME - UV.y - UV.x), sin(TIME - UV.y), sin(TIME - UV.x), 1.0);
	}
}"

